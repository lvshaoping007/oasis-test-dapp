{
  "name": "test-dapp",
  "version": "1.0.0",
  "description": "A simple dapp used in Oasis e2e tests.",
  "engines": {
    "node": ">= 12.0.0"
  },
  "scripts": {
    "lint": "eslint . --ext js,json && prettier ./src -c",
    "lint:fix": "eslint . --fix --ext js,json && prettier ./src -c --write",
    "start": "webpack serve",
    "build": "webpack --mode production",
    "test": "yarn lint",
    "prepublishOnly": "yarn test && yarn build",
    "deploy": "gh-pages -d dist"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git"
  },
  "files": [
    "dist/"
  ],
  "author": "",
  "license": "MIT",
  "homepage": "./",
  "devDependencies": {
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.0.2",
    "eslint": "^7.7.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-json": "^2.0.1",
    "eslint-plugin-node": "^11.1.0",
    "prettier": "^2.0.5",
    "webpack": "^5.45.1",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^4.0.0-rc.0",
    "@oasisprotocol/client": "0.1.0-alpha5",
    "@oasisprotocol/client-rt": "0.2.0-alpha3",
    "@oasisprotocol/client-ext-utils": "0.1.0-alpha2",
    "stream-browserify": "^3.0.0",
    "buffer": "6.0.3",
    "process": "^0.11.10"
  }
}
